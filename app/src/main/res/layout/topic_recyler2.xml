<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"

    xmlns:app="http://schemas.android.com/apk/res-auto">


    <LinearLayout
        android:id="@+id/touchh"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginHorizontal="10dp"
        android:gravity="center_vertical"
        android:background="@drawable/btn_bg_white"
        android:orientation="horizontal">
        <ImageButton
            android:id="@+id/button3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/btn_bg_blacck"
            android:gravity="center"

            android:layout_marginLeft="10dp"
            android:padding="3dp"
            app:srcCompat="@drawable/indiacotor_inactive_onboard"
            />
        <TextView
            android:id="@+id/topic555"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"

            android:textColor="@color/white"
            android:textSize="18dp"
            android:textStyle="bold"
            android:fontFamily="@font/font"
            android:text="Html 5 web devlopement"/>


    </LinearLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/obRR"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:listitem="@layout/objective_recycler"/>

</LinearLayout>